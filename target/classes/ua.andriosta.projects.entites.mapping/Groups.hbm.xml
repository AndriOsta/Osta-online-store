<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.andriosta.projects.ostastore.entites.Groups" table="groups" schema="" catalog="ostastore">
        <id name="id">
            <column name="id" sql-type="bigint assigned" not-null="true"/>
        </id>
        <property name="groupName">
            <column name="group_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>