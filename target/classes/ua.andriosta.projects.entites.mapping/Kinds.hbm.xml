<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.andriosta.projects.ostastore.entites.Kind" table="kinds" schema="" catalog="ostastore">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <set name="goodsesById" inverse="true">
            <key>
                <column name="kind_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ua.andriosta.projects.ostastore.entites.Goods"/>
        </set>
    </class>
</hibernate-mapping>