<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.andriosta.projects.ostastore.entites.Users" table="users" schema="" catalog="ostastore">
        <id name="username">
            <column name="username" sql-type="varchar" length="45" not-null="true"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <property name="enabled">
            <column name="enabled" sql-type="tinyint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>